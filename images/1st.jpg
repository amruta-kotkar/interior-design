    .acc {
  margin: 0 auto;
  max-width: 800px;
}
.acc__card {
  margin: 3px;
  position: relative;
  background-color:#fafafa;
}
.acc__title:before {
    content: '\f067';
    float: right;
    cursor: pointer;
    font: normal normal normal 14px/1 FontAwesome;
}
.acc__title.active:before {
    content: "\f068";
    font: normal normal normal 14px/1 FontAwesome;
    float: right;
    cursor: pointer;
}
.acc__title {
  background: $color-wht;
  box-shadow: 0 0 20px rgba($color-blk, .2);
  color: $color-gry;
  cursor: pointer;
  display: block;
  padding: 1em 1.5em;
  position: relative;
  text-align: left;
  &::after {
    width: 8px;
    height: 8px;
    border-right: 1px solid #4a6e78;
    border-bottom: 1px solid #4a6e78;
    position: absolute;
    right: 10px;
    content: " ";
    top: 17px;
    transform: rotate(-45deg);
    transition: all 0.2s ease-in-out;
  }
  &.active::after {
    transform: rotate(45deg);
    transition: all 0.2s ease-in-out;
  }
}
.acc__panel {
  background: $color-wht;
  color: $color-gry;
  display: none;
  margin: 0;
  padding: 2em;
  text-align: left;
}
@media screen and (max-width: 480px){
     .flip-card{margin: 20px auto;}
}
@media screen and (min-width: 767px){
     .flip-card{height:585px;}
}

 .flip-card { background-color: #d0ad55;
  width: 300px;
 /* height: 350px;*/
  perspective: 1000px;padding: 30px 10px;
}

.flip-card-inner {
  position: relative;
  width: 100%;
  height: 100%;
  text-align: center;
 /* transition: transform 0.6s;
  transform-style: preserve-3d;
  box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);*/
}

.flip-card:hover .flip-card-inner {
 /* transform: rotateY(180deg);*/
}

.flip-card-front, .flip-card-back {
  /*position: absolute;*/position:relative;
  width: 100%;
  height: 100%;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}

.flip-card-front {
  background-color: #d0ad55;
  color: black;
}
.flip-card-front h3{margin-top: 13px;
